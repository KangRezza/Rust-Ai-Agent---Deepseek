DEEPSEEK_API_KEY=
DEEPSEEK_BASE_URL=https://api.deepseek.com
DEEPSEEK_MODEL=deepseek-chat
DEEPSEEK_MAX_TOKENS=2048
DEEPSEEK_TEMPERATURE=0.7

# Twitter Configuration for agent-twitter-client
TWITTER_USERNAME=
TWITTER_PASSWORD=

# Twitter cookie string for authentication
TWITTER_COOKIE_STRING=

# twitter configuration
TWEET_FREQUENCY_MIN=30      # Minutes between tweets
MAX_TWEETS_PER_DAY=10        # Maximum tweets per day
ENABLE_REPLIES=true          # Enable replying to tweets
ENABLE_RETWEETS=true         # Enable retweet



# Crawler Configuration
ENABLE_CRAWLER=true
